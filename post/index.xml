&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="k">parameter&lt;/span> &lt;span class="n">PERIOD&lt;/span> &lt;span class="o">=&lt;/span> &lt;span class="mh">10&lt;/span>&lt;span class="p">;&lt;/span>
